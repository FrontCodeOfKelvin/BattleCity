{
  "extends": ["tslint-microsoft-contrib"],
  "rules": {
    "no-console": false,
    "missing-jsdoc": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "no-import-side-effect": [true, {"ignore-module": "(\\.html|\\.css)$"}],
    "no-relative-imports": false
  }
}
